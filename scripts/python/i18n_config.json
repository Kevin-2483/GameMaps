{
  "api_settings": {
    "url": "https://api.deepseek.com/chat/completions",
    "model": "deepseek-chat",
    "temperature": 0.1,
    "max_tokens": 4000,
    "top_p": 0.95,
    "frequency_penalty": 0.0,
    "presence_penalty": 0.0
  },
  "processing_settings": {
    "max_concurrent": 100,
    "retry_attempts": 3,
    "retry_delay": 2.0,
    "timeout": 120,
    "validate_line_count": true
  },
  "output_settings": {
    "arb_base_path": "lib/l10n",
    "backup_original": false,
    "validate_line_count": true,
    "arb_file_extension": ".arb",
    "preserve_directory_structure": true
  }
}